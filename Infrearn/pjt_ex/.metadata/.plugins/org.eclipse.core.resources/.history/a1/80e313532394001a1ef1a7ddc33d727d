package inherit;

public class FirstChaildClass extends ParentClass {
	
	public FirstChaildClass() {
		System.out.println("--ChildClass Constructor--");
	}
	
	public void childFun() {
		System.out.println("childFun()");
	}
	
	@Override
	public void makeJajang() {
		System.out.println(" -- more delicious makeJajang --");
	}
}
