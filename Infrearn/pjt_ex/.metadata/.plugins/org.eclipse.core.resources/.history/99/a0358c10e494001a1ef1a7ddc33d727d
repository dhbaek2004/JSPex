package NetworkServer;

import java.io.IOException;
import java.net.Socket;

public class MainClassSocket {

	public static void main(String[] args) {
		
		Socket socket = null;
		
		try {
			socket = new Socket("localhost", 9000); //127.0.0.1
		} catch (IOException e) {
			e.printStackTrace();
		}

	}

}
